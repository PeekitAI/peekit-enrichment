# AWS Configuration
AWS_REGION=ap-south-1
ATHENA_S3_OUTPUT=s3://peekit-athena-results-126730103313/output/
ATHENA_DATABASE=peekit_crawlers
ATHENA_WORKGROUP=peekit-crawlers

# Z.ai API Configuration
ZAI_API_KEY=your_zai_api_key_here
ZAI_API_URL=https://api.z.ai/v1/chat/completions
ZAI_MODEL=gpt-4o-mini

# Enrichment Configuration
ENRICHMENT_LIMIT=100
ENRICHMENT_BATCH_SIZE=10

# Provider Selection (comma-separated, defaults to all)
# Available: x_tweets, apify_x_tweets, apify_tiktok_posts, instagram_posts, reddit_posts, tiktok_videos, youtube_videos
ENRICHMENT_PROVIDERS=

# Module Toggles
ENABLE_SENTIMENT=true
ENABLE_ENTITY=true
ENABLE_TOPIC=true
ENABLE_ENGAGEMENT=true
ENABLE_MODERATION=true
